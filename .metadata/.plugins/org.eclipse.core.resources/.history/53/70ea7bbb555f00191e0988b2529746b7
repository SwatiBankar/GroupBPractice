package AbstractionProgram;

public class companyEmployee {
	String name;
	String address;
	float salary;
public companyEmployee(String name,String address,float salary)
    {
	   System.out.print("this is a constroctor");
	   this.name=name;
	   this.address=address;
	   this.salary=salary;
       	
	}
public String toString()
{
	
   return(name+""+address+""+salary+"");	
}






}



