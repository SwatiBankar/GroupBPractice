		// TODO Auto-generated method stub
//int i=4;
char ch='a';
int i;
   for( i=0;i<=4;i++)
   {
	   for(int space=4;space>=i;space--)
	   {
		   System.out.print(" ");
	   }
    
   for(int j=0;j<=i;j++)
   {
	   if(j==0)
	   {
	      System.out.print("*"+" ");
       }
	   if(j==1)
	   {
		   System.out.print(ch+" ");
		   ch++;
	   }
	   else if(j==2)
	   {
		   System.out.print(j);
	   }
   }
  }
}

}

   
		   
		   
	

	



