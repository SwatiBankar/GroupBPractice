
public class FirstLetterReplaceString {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s="i like testing shastra";
		String[] s1=s.split("");
		String s2="";
		String tempt;
		for(int i=0;i<=s1.length;i++)
		{
			 tempt=tempt+s1[i];
		}
		System.out.print(tempt);
	}
}

			
		
			
			
			
			
			
			
			
			
			
			
			
			
			
			
			
			
			
			
			
			
			
			