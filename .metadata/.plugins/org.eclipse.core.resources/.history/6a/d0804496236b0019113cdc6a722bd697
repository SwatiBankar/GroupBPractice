
public class Demo2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num=1;
		char ch='B';
		for(int i=0;i<=4;i++)
		{
			for(int j=0;j<=i;j++)
			{
				if(i%2==0)
				{
					System.out.print(num+" ");
				}
				else
				{
					System.out.print(ch+" ");
				}
				
			}
			System.out.println();
			
		}
		

	}

}
