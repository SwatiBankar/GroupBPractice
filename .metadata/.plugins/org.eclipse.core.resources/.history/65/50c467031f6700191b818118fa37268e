
public class StarNew1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int i=4;
char ch='a';
int j;
   for( j=0;j<=i;j++)
   {
	   for(int space=4;space>=j;space--)
	   {
		   System.out.print(" ");
	   }
	   
   }
   if(j==0)
   {
	   System.out.print("*");
   }
   else if(j==1)
   {
	   System.out.println(ch);
	   ch++;
   }
   else if(j==2)
   {
	   System.out.print(j);
	   
   }
}
	
   
	}


