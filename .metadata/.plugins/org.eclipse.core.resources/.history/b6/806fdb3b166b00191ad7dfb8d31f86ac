
public class StarProgram {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n=10;
      for(int i=0;i<=n;i++)
      {
    	  for(int space=0;space<=i;space++)
    	  {
    		  System.out.print(" ");
    	  }
    	  
    	 for(int j=0;j<=i;j++)
    		 
    	 {
    		System.out.print(j+"");
    		
    	 }
    	 
    	 
      
	}
      //System.out.println();
      
	
	}
	 
}
