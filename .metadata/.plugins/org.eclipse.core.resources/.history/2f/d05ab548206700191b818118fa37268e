
public class StarNew1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
//int i=4;
//char ch='a';
int i;
   for( i=0;i<=4;i++)
   {
	   for(int space=4;space>=i;space--)
	   {
		   System.out.print(" ");
	   }
    
   for(int j=0;j<=i;j++)
   {
	   System.out.print("*"+" ");
   }
   System.out.println();

   }
}
	
}

	



