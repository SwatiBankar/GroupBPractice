
public class StarProgram {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
      for(int i=0;i<=4;i++)
      {
    	  for(int space=4;space>=i;space--)
    	  {
    		  System.out.print(" ");
    	  }
        
    	 for(int j=0;j<=i;j++)
    		 
    	 {
    		System.out.print(i+" ");
    	
    		
    	 }
    	 
    	 System.out.println();
    	 
	}
      
      
      
	}
	}
	 
